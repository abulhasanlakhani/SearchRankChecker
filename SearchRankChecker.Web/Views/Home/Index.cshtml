@model string
@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h1 class="display-4">Search Rank Checker</h1>
    <p>Enter your site url and search terms to see where your site ranks in the search.</p>
</div>

<partial name="SearchRank" model="new Search()" />

@if (!string.IsNullOrEmpty(Model))
{
    <div class="container">
        <div class="row">
            <div class="col-12">
                <h2>Search Rank:</h2>
                <p>@Model</p>
            </div>
        </div>
    </div>
}

